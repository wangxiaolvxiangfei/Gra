            <div class="templatemo-content-container now-position ly-tab">
                <!--  <span>当前位置：</span><span>管理员登录>></span><span>本科转生求职与就业管理>></span><span><a href="StudentJobInformation.html">就业签约管理>></a></span><span>无协议书就业</span> -->
                <div class="content">
                    @using (@Html.BeginForm("EditApplExam", "Admin", FormMethod.Post))
                    {
                        @Html.HiddenFor(model=>model.applInfo.StudentNumber)
                        @Html.AntiForgeryToken()
                        @Html.ValidationSummary(true)


                    <table class="informationTable table">
                        <thead>
                            <tr>
                                <td colspan="8" style="text-align:center">
                                    <b>
                                        华北电力大学本专科毕业生求职信息（推荐表）
                                    </b>
                                </td>
                            </tr>
                        </thead>
                        <tr>
                            <td rowspan="2" style="width:10px; border-right:1px solid black">毕业生信息</td>
                            <td><span>姓名：</span><span>@Html.ValueFor(model => model.upload.Name)</span></td>
                            <td><p>学号：</p><span>@Html.ValueFor(model => model.upload.StudentNumber)</span></td>
                            <td><p>出生年月日：</p><span>@Html.ValueFor(model => model.upload.BirthTime)</span></td>
                            <td>
                                <p>政治面貌：</p>
                                @ViewBag.P
                            </td>
                            <td>
                                <p>健康状况：</p>@ViewBag.H
                            </td>
                            <td>
                                <p>邮政编码</p>@ViewBag.E
                            </td>
                        </tr>
                        <tr>
                            <td colspan="2">
                                <p>通讯地址：</p>@ViewBag.CA
                            </td>
                            <td colspan="2">
                                <p>现家庭居住地：</p>@ViewBag.F
                            </td>
                            <td>
                                <p>Email：</p>@ViewBag.E
                            </td>
                            <td>
                                <p>联系电话：</p>@ViewBag.T
                            </td>
                        </tr>
                        <tr>
                            <td rowspan="9" style="width:10px; border-right:1px solid black">教育背景</td>
                            <td colspan="2">
                                <span>专业：</span><span>@Html.ValueFor(model => model.upload.MajorDirection)</span>
                            </td>
                            <td>
                                <span>学历：</span><span>@Html.ValueFor(model => model.upload.Education)</span>
                            </td>
                            <td>
                                <span>培养方式：</span><span>@Html.ValueFor(model => model.upload.TrainType)</span>
                            </td>
                            <td>
                                <span>毕业时间：</span><span>@Html.ValueFor(model => model.upload.GraduationTime)</span>
                            </td>
                            <td>
                                <p>成绩班级名次</p>
                                @Html.TextBoxFor(model => model.applInfo.Rank)
                            </td>
                        </tr>
                        <tr style="text-align:center">
                            <td><b>教育背景</b></td>
                        </tr>
                        <tr>
                            <td colspan="4">
                                <p>第一年度获奖（评优，荣誉，奖学金）</p>
                                @Html.TextAreaFor(model => model.applInfo.FristYearPrize)
                            </td>
                        </tr>
                        <tr>
                            <td colspan="4">
                                <p>第二年度获奖（评优，荣誉，奖学金）</p>
                                @Html.TextAreaFor(model => model.applInfo.SecondYearPrize)
                            </td>
                        </tr>
                        <tr>
                            <td colspan="4">
                                <p>第三年度获奖（评优，荣誉，奖学金）</p>
                                @Html.TextAreaFor(model => model.applInfo.ThirdYearPrize)
                            </td>
                        </tr>
                        <tr>
                            <td colspan="4">
                                <p>2.竞赛获奖情况：</p>
                                @Html.TextAreaFor(model => model.applInfo.CompetitionPrize)
                            </td>
                        </tr>
                        <tr>
                            <td colspan="4">
                                <p>3.其他获奖情况：</p>
                                @Html.TextAreaFor(model => model.applInfo.OtherPrize)
                            </td>
                        </tr>
                        <tr>
                            <td colspan="4">
                                <p>4.发表论文和学术成果：</p>
                                @Html.TextAreaFor(model => model.applInfo.PublishPaper)
                            </td>
                        </tr>
                    </table>
                        //本科生能力特长
                        <table class="informationTable table" @ViewBag.numbercount>
                            <tr style="text-align:center">
                                <td><b>能力特长</b></td>
                            </tr>
                            <tr>
                                <td colspan="4">
                                    <span>外语语种：英语通过级别：</span>
                                    @Html.DropDownListFor(model => model.applInfo.EnglishLevel, ViewBag.english as List<SelectListItem>
                                    )
                                    <span>四级成绩：</span>@Html.TextBoxFor(model => model.applInfo.LevelFour)
                                    <span>六级成绩：</span>@Html.TextBoxFor(model => model.applInfo.LevelSix)
                                </td>
                            </tr>
                            <tr>
                                <td colspan="4">
                                    <span>计算机级别：</span>@Html.TextBoxFor(model => model.applInfo.ComputerLevel)
                                </td>
                            </tr>
                        </table>
                        //研究生能力特长
                        <table class="informationTable table" @ViewBag.type>
                            <tr style="text-align:center">
                                <td><b>能力特长</b></td>
                            </tr>
                            <tr>
                                <td colspan="4">
                                    1.主修外语注：非英英语专业填写"CET-4","CET-6";英语专业填写"TEM-4","TEM-8","无",其他语种毕业生填写“——”
                                </td>
                            </tr>
                            <tr>
                                <td colspan="4">
                                    语种：语言考试分数：@Html.TextBoxFor(model => model.applInfo.LanguageScore)
                                </td>
                            </tr>
                            <tr>
                                <td colspan="4">
                                    <span>计算机水平：</span>@Html.TextAreaFor(model => model.applInfo.ComputerLevel)
                                </td>
                            </tr>
                            <tr>
                                <td colspan="4">
                                    <span>特长能力：</span>@Html.TextAreaFor(model => model.applInfo.SpecialtyAbilty)
                                </td>
                            </tr>
                            <tr>
                                <td colspan="4">
                                    <span>社会工作：</span>@Html.TextAreaFor(model => model.applInfo.SocietyJob)
                                </td>
                            </tr>
                            <tr>
                                <td colspan="4">
                                    <span>实践活动：</span>@Html.TextAreaFor(model => model.applInfo.PracticeActivity)
                                </td>
                            </tr>
                        </table>


                        <table class="informationTable table">
                            <tr style="text-align:center">
                                <td><b>求职备注</b></td>
                            </tr>
                            <tr>
                                <td colspan="4">
                                    @Html.TextAreaFor(model => model.applInfo.ApplNote)
                                </td>
                            </tr>
                            <tr>
                                <td colspan="4">
                                    <input type="submit" value="提交" class="btn btn-warning">
                                </td>
                            </tr>
                        </table>
                   
                @*本科生*@
                <table class="informationTable table" @ViewBag.numbercount>
                    <tr style="text-align:center">
                        <td colspan="4">
