                <h1 class="ly-title">毕业生信息导入</h1>
                @using (Html.BeginForm("Upload", "Admin", FormMethod.Post, new { enctype = "multipart/form-data" ,@class="form-horizontal"}))
                {
                    <div class="form-group">
                        <div class="col-sm-offset-2 col-sm-10">
                            <div class="radio">
                                <label>
                                    @Html.RadioButtonFor(m=>m.StudentType,"1") <b>本科生</b>
                                    @Html.RadioButtonFor(m=>m.StudentType,"2") <b>研究生</b>
                                </label>
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-sm-3 col-sm-offset-2">
                            <input type="file" name="uploadFile">
                        </div>
                        <div class="col-sm-4">
                            <button class="btn btn-info">上传</button>
                            @if (Model != null)
                            { 
                                @Html.ActionLink("上传到数据库", "Upload", new { type = "1", studentTyppe = @Model.StudentType })
                            }
                        </div>
                    </div> 
                    <div>
                        <span>@ViewBag.messagge</span>
                    </div>
                }
