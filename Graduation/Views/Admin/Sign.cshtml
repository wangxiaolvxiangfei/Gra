            <div class="templatemo-top-nav-container">
                <h2>求职与就业信息管理系统</h2>
                <div class="right">
                    <span>欢迎&nbsp;&nbsp;</span><span class="admin">就业中心管理员：</span><span class="admin">Admin</span><span>&nbsp;&nbsp;登录本系统</span>
                </div>
            </div>
            <div class="templatemo-content-container now-position">
                <span>当前位置：</span><span>毕业生>></span><span>签约登记表</span><span></span>
                <h1 class="ly-title">签约登记表</h1>
                @using (@Html.BeginForm(null, null, null, FormMethod.Post, new { @class = "form-horizontal", style = "text-align:center" }))
                {
                    @Html.AntiForgeryToken()
                    @Html.ValidationSummary(true)
                    <div class="form-group">
                        <div>
                            <div>
                                <label>
                                    @Html.RadioButtonFor(model => model.Upload.StudentType, "0") 本科生
                                    @Html.RadioButtonFor(model => model.Upload.StudentType, "1") 研究生
                                </label>
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <div>
                            <label>姓名：</label>
                            @Html.TextBoxFor(model => model.Upload.Name)
                        </div>
                    </div>
                    <div class="form-group">
                        <div>
                            <label>学号：</label>
                            @Html.TextBoxFor(model => model.Upload.StudentNumber)
                        </div>
                    </div>
                    <button class="btn btn-info" style="width:190px">查询</button>
                }
                @if (Model != null)
                {
                    @Html.ActionLink("下载EXCEL表格", "Sign", new { type = 1 });
                    <div class="panel panel-default table-responsive">
                        <table class="table table-striped table-bordered templatemo-user-table table-hover .houtai-table select">
                            <thead>
                                <tr>
                                    <th>编辑</th>
                                    <th>学号</th>
                                    <th>姓名</th>
                                    <th>学历</th>
                                    <th>院</th>
                                    <th>系</th>
                                    <th>班级</th>
                                    <th>登记日期</th>
                                    <th>单位名称</th>
                                    <th>隶属部门</th>
                                    <th>单位性质</th>
                                    <th>单位地址</th>
                                    <th>单位联系人</th>
                                    <th>单位电话</th>
                                    <th>个人联系方式</th>
                                    <th>签约方式</th>
                                    <th>协议书编号</th>
                                    <th>所属集体或系统</th>
                                </tr>

                            </thead>
                            @foreach (var item in Model.SignList)
                            {
                                <tr>
                                    <td>@Html.ActionLink("编辑", "EditSign", new { studentNumber = @item.upload.StudentNumber })</td>
                                    <td>@item.upload.StudentNumber</td>
                                    <td>@item.upload.Name</td>
                                    <td>@item.upload.Education</td>
                                    <td>@item.upload.Academy</td>
                                    <td>@item.upload.Department</td>
                                    <td>@item.upload.Class</td>
                                    @if (@item.signInfo != null)
                                    {
                                        <td>@item.signInfo.SignTime</td>
                                        <td>@item.signInfo.CompanyName</td>
                                        <td>@item.signInfo.ComBelongDep</td>
                                        <td>@item.signInfo.ComType</td>
                                        <td>@item.signInfo.CompanyAddress</td>
                                        <td>@item.signInfo.CompanyConn</td>
                                        <td>@item.signInfo.CompanyTel</td>
                                        <td>@item.signInfo.PerTelType</td>
                                        <td>@item.signInfo.SignType</td>
                                        <td>@item.signInfo.AgreementID</td>
                                        <td>@item.signInfo.ComBelongGroup</td>
                                    }
                                    else
                                    {
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                    }
                                </tr>
                            }
                        </table>
                    </div>
                }
                <footer class="text-right"></footer>
            </div>
