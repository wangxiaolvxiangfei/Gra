                                <td colspan="2">
                                    <span>通讯地址：</span>
                                    @Html.TextBoxFor(model => model.fillBaseInfoModel.CommAddress)
                                </td>
                            </tr>
                            <tr>
                                <td><span>家庭邮编：</span>@Html.TextBoxFor(model => model.fillBaseInfoModel.FamilyZipCode)</td>
                                <td colspan="2">
                                    <span>家庭详细地址(具体到门牌号农村的行政村级)：</span>
                                    @Html.TextBoxFor(model => model.fillBaseInfoModel.FamilyLocation)
                                </td>
                            </tr>
                            <tr>
                                <td><span>父亲姓名：</span>@Html.TextBoxFor(model => model.fillBaseInfoModel.FatherName)</td>
                                <td colspan="2">
                                    <span>父亲工作地址：</span>
                                    @Html.TextBoxFor(model => model.fillBaseInfoModel.FatherWorkUnit)
                                </td>
                            </tr>
                            <tr>
                                <td><span>母亲姓名：</span>@Html.TextBoxFor(model => model.fillBaseInfoModel.MotherName)</td>
                                <td colspan="2">
                                    <span>母亲工作地址：</span>
                                    @Html.TextBoxFor(model => model.fillBaseInfoModel.MotherWorkUnit)
                                </td>
                            </tr>
                            <tr>
                                <td colspan="4">
                                    <p>备注：</p>
                                    @Html.TextAreaFor(model => model.fillBaseInfoModel.BaseNote)
                                </td>
                            </tr>

                            <tr>
                                <td colspan="3" align="center">
                                    <button class="btn btn-info">提交</button>
                                </td>
                            </tr>
                        </table>
