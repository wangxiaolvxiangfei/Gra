@model Graduation.Models.ESchoolInfoViewModel

@if (ViewBag.clocked == "锁定")
{
    ViewBag.clocked = "disabled";
}


<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>本科生就业信息管理</title>
    <meta name="description" content="">
    <meta name="author" content="templatemo">
    <script src="~/Scripts/City.js"></script>
</head>
<body>
    <!-- Left column -->
    <div class="templatemo-flex-row">
        <div class="templatemo-sidebar">
            <header class="templatemo-site-header">
                <h1>华北电力大学</h1>
            </header>
            <div class="profile-photo-container">
                <img src="../../images/profile-photo.jpg" alt="Profile Photo" class="img-responsive">
                <div class="profile-photo-overlay"></div>
            </div>
            <div class="mobile-menu-icon">
                <i class="fa fa-bars"></i>
            </div>
            <nav class="templatemo-left-nav">
                <ul class="ly-ul-parent">
                    <li>
                        <a href="/FillBaseInfo/Index" class="active">
                            <i class="fa fa-home fa-fw"></i>
                            通知
                        </a>
                    </li>
                    <li>
                        <a href="/FillBaseInfo/FillBaseInfo">
                            <i class="fa fa-bar-chart fa-fw"></i>
                            基本信息管理
                        </a>
                    </li>
                    <li>
                        <a href="/FillBaseInfo/FillapplInfo">
                            <i class="fa fa-database fa-fw"></i>
                            求职信息管理
                        </a>
                    </li>
                    <li>
                        <a href="/FillBaseInfo/EmployInfo">
                            <i class="fa fa-map-marker fa-fw"></i>
                            就业信息管理
                        </a>
                    </li>
                    <li>
                        <a href="/FillBaseInfo/FillSignInfo">
                            <i class="fa fa-users fa-fw"></i>
                            签约登记管理
                        </a>
                    </li>
                    <li>
                        <a href="StudentLogin.html">
                            <i class="fa fa-sliders fa-fw"></i>
                            修改密码
                        </a>
                    </li>
                    <li>
                        <a href="StudentImg.html">
                            <i class="fa fa-eject fa-fw"></i>
                            上传照片
                        </a>
                    </li>
                    <li>
                        <a href="StudentLogin.html">
                            <i class="fa fa-user-md fa-fw"></i>
                            退出系统
                        </a>
                    </li>
                </ul>
            </nav>
        </div>
        <!-- Main content -->
        <div class="templatemo-content col-1 light-gray-bg">
            <div class="templatemo-top-nav-container">
                <h2>求职与就业信息管理系统</h2>
                <div class="right">
                    <span>用户信息：</span><span>学号：</span><span>&nbsp;&nbsp;</span><span class="admin">@Html.ValueFor(model => model.upload.StudentNumber)</span>
                </div>
            </div>
            <div class="templatemo-content-container now-position ly-tab">
                <span>当前位置：</span><span>毕业生登录>></span><span>本科转生求职与就业管理>></span><span><a href="StudentJobInformation.html">就业签约管理>></a></span><span>有协议书就业</span>
                @using (@Html.BeginForm())
                {
                    @Html.HiddenFor(model => model.eSchoolInfo.StudentNumber)
                    @Html.AntiForgeryToken()
                    @Html.ValidationSummary(true)
                    <div class="content">
                        <span class="marginLeft">允许操作时间：</span><span>起始时间：</span><span class="admin">2015/10/18 0:00:00</span><span>结束时间：</span><span class="admin">2016/8/5 23:59:34</span>
                        <table class="informationTable table">
                            <thead>
                                <tr>
                                    <td colspan="4" style="text-align:center"><b>个人信息</b></td>
                                </tr>
                            </thead>
                            <tr>
                                <td><span>学号：</span><span>@Html.ValueFor(model => model.upload.StudentNumber)</span></td>
                                <td><span>姓名：</span><span>@Html.ValueFor(model => model.upload.Name)</span></td>
                                <td><span>性别：</span><span>@Html.ValueFor(model => model.upload.Sex)</span></td>
                                <td><span>出生年月：</span><span>@Html.ValueFor(model => model.upload.BirthTime)</span></td>
                            </tr>
                            <tr>
                                <td><span>政治面貌：</span><span>@ViewBag.P</span></td>
                                <td><span>专业：</span><span>@Html.ValueFor(model => model.upload.Major)</span></td>
                                <td><span>培养方式：</span><span>@Html.ValueFor(model => model.upload.TrainType)</span></td>
                                <td><span>学历：</span><span>@Html.ValueFor(model => model.upload.Education)</span></td>
                            </tr>
                            <tr>
                                <td><span>学制：</span><span>@Html.ValueFor(model => model.upload.LengthOfSch)</span></td>
                                <td><span>毕业时间：</span><span>@Html.ValueFor(model => model.upload.GraduationTime)</span></td>
                                <td><span>生源地区：</span><span>@ViewBag.L</span></td>
                                <td><span>身份证号：</span><span>@Html.ValueFor(model => model.upload.IDNumber)</span></td>
                            </tr>
                            <tr>
                                <td><span>健康状况：</span><span>@ViewBag.H</span></td>
                                <td><span>手机号码：</span><span>@ViewBag.T</span></td>
                                <td><span>邮政编码：</span><span>@ViewBag.C </span></td>
                                <td><p></p><span></span></td>
                            </tr>
                            <tr>
                                <td colspan="2" class="input-width"><p>通信地址：</p><span>@ViewBag.ADDR</span></td>
                                <td colspan="2" class="input-width"><p>家庭所在地：</p><span>@ViewBag.FL</span></td>
                            </tr>
                            <tr>
                                <td colspan="2" class="input-width"><p>协议书编号：</p>@Html.TextBoxFor(model => model.eSchoolInfo.AgreementID)*唯一</td>
                            </tr>
                            <tr>
                                <td>
                                    <p>单位名称：</p>
                                    @Html.TextBoxFor(model => model.eSchoolInfo.CompanyName)
                                </td>
                                <td>
                                    <p>单位所属集团或系统</p>
                                    @Html.DropDownListFor(model => model.eSchoolInfo.ComBelongGroup, ViewBag.Group as List<SelectListItem>)
                                </td>
                                <td>
                                    <p>单位组织机构代码</p>
                                    <p class="warn">(9位数字或字母不含‘-’)</p>
                                    @Html.TextBoxFor(model => model.eSchoolInfo.CompanyCode)
                                </td>
                                <td>
                                    <p>单位上级隶属部门</p>
                                    @Html.DropDownListFor(model => model.eSchoolInfo.ComBelongDep, ViewBag.belongDep as List<SelectListItem>)
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <p>单位行业</p>
                                    @Html.DropDownListFor(model => model.eSchoolInfo.ComIndustry, ViewBag.hangye as List<SelectListItem>)
                                </td>
                                <td>
                                    <p>工作职务</p>
                                    @Html.DropDownListFor(model => model.eSchoolInfo.JobTitle, ViewBag.zhiwei as List<SelectListItem>)
                                </td>
                                <td colspan="2" class="input-width">
                                    <p>《就业报到证》开具单位名称</p>
                                    @Html.TextBoxFor(model => model.eSchoolInfo.SCompanyName)
                                </td>
                            </tr>
                            <tr>
                                <td colspan="2" class="input-width">
                                    <p>单位所在地</p>
                                    @Html.TextBoxFor(m => m.eSchoolInfo.ComLocation, new { id = "originPro", onkeyup = "GetCity()" })
                                    @Html.DropDownListFor(m => m.eSchoolInfo.ComLocationCode, ViewBag.city as List<SelectListItem>, new { id = "originList" })
                                </td>
                                <td colspan="2">
                                    <p>单位性质：</p>
                                    @Html.RadioButtonFor(model => model.eSchoolInfo.ComType, "10机关")机关
                                    @Html.RadioButtonFor(model => model.eSchoolInfo.ComType, "20科研设计单位")科研设计单位
                                    @Html.RadioButtonFor(model => model.eSchoolInfo.ComType, "21高等教育单位")高等教育单位
                                    @Html.RadioButtonFor(model => model.eSchoolInfo.ComType, "22中初教育单位")中初教育单位
                                    @Html.RadioButtonFor(model => model.eSchoolInfo.ComType, "23医疗卫生单位")医疗卫生单位
                                    @Html.RadioButtonFor(model => model.eSchoolInfo.ComType, "29其他事业单位")其他事业单位
                                    @Html.RadioButtonFor(model => model.eSchoolInfo.ComType, "31国有企业")国有企业
                                    @Html.RadioButtonFor(model => model.eSchoolInfo.ComType, "32三资企业")三资企业
                                    @Html.RadioButtonFor(model => model.eSchoolInfo.ComType, "39其他企业")其他企业
                                    @Html.RadioButtonFor(model => model.eSchoolInfo.ComType, "40部队")部队
                                    @Html.RadioButtonFor(model => model.eSchoolInfo.ComType, "55农村建设")农村建设
                                    @Html.RadioButtonFor(model => model.eSchoolInfo.ComType, "56城镇社区")城镇社区
                                    @Html.RadioButtonFor(model => model.eSchoolInfo.ComType, "99其他")其他
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <p>联系人：</p>@Html.TextBoxFor(model => model.eSchoolInfo.Contacts)
                                </td>
                                <td>
                                    <p>联系人邮编(6位数字)：</p>@Html.TextBoxFor(model => model.eSchoolInfo.ContactsCode)
                                </td>
                                <td colspan="2" class="input-width">
                                    <p>联系人通信地址：</p>@Html.TextBoxFor(model => model.eSchoolInfo.ConCommAddress)
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <p>联系人职务：</p>@Html.TextBoxFor(model => model.eSchoolInfo.ConPost)
                                </td>
                                <td>
                                    <p>联系人电话：</p>@Html.TextBoxFor(model => model.eSchoolInfo.ConTel)
                                </td>
                                <td colspan="2" class="input-width">
                                    <p>户口迁移地址：</p>@Html.TextBoxFor(model => model.eSchoolInfo.ResChangedAdd)
                                </td>
                            </tr>
                            <tr>
                                <td colspan="2" class="input-width">
                                    <p>档案转寄详细地址：</p>@Html.TextBoxFor(model => model.eSchoolInfo.FileChangedAdd)
                                </td>
                                <td>
                                    <p>邮政编码(6位数字)：</p>@Html.TextBoxFor(model => model.eSchoolInfo.FileZipCode)
                                </td>
                                <td>
                                    <p>是否专业对口：</p>
                                    @Html.RadioButtonFor(model => model.eSchoolInfo.IsMajorFit, "1") 是
                                    @Html.RadioButtonFor(mdoel => mdoel.eSchoolInfo.IsMajorFit, "0") 否
                                </td>
                            </tr>
                            <tr>
                                <td colspan="4">
                                    <p>备注：</p>
                                    @Html.TextAreaFor(model => model.eSchoolInfo.Note)
                                </td>
                            </tr>
                            <tr>
                                <td colspan="4">
                                    <input type="submit" value="提交" class="btn btn-info" @ViewBag.clocked />
                                </td>
                            </tr>
                        </table>
                    </div>
                }
            </div>
        </div>
    </div>
</body>
</html>